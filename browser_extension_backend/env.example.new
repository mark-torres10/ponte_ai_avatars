# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here
REALTIME_MODEL=gpt-realtime
REALTIME_VOICE=verse
TOKEN_TTL_SECONDS=600

# Security Configuration - JSON array format for lists
ALLOWED_ORIGINS=["https://www.espn.com","chrome-extension://your-extension-id"]
CORS_ORIGINS=["https://www.espn.com","chrome-extension://your-extension-id"]

# Rate Limiting
RATE_LIMIT_PER_MINUTE=10
RATE_LIMIT_BURST=5

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Advanced Settings
DEFAULT_INSTRUCTIONS="You are Parker, an enthusiastic sports commentator. Respond with passion and energy, matching the user's intensity level."
DEFAULT_TEMPERATURE=0.8
DEFAULT_MAX_TOKENS=4096
DEFAULT_SPEED=1.0

# Monitoring
HEALTH_CHECK_INTERVAL=30
METRICS_ENABLED=true

# Application
DEBUG=false
