# Server Configuration
NODE_ENV=development
PORT=3001

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# API Keys (REQUIRED for video generation)
OPENAI_API_KEY=your_openai_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
DID_API_KEY=your_did_api_key_here

# Supabase Configuration (REQUIRED for file storage)
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ElevenLabs Voice IDs (REQUIRED for voice generation)
# TODO: These will eventually be fetched from Supabase database instead of environment variables
ELEVENLABS_TERRY_CREWS_VOICE_ID=your_terry_crews_voice_id_here
ELEVENLABS_WILL_HOWARD_VOICE_ID=your_will_howard_voice_id_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info

# IMPORTANT: Copy this file to .env and fill in your actual API keys
# The following services are required for full functionality:
# 1. ElevenLabs - for voice generation
# 2. D-ID - for video generation  
# 3. Supabase - for file storage 