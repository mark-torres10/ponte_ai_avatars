# Server Configuration
NODE_ENV=development
PORT=3001

# CORS Configuration
CORS_ORIGIN=https://ponte-ai-avatars.vercel.app

# API Keys (to be provided later)
OPENAI_API_KEY=your_openai_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
DID_API_KEY=your_did_api_key_here

# ElevenLabs Voice IDs (to be provided later)
# TODO: These will eventually be fetched from Supabase database instead of environment variables
ELEVENLABS_TERRY_CREWS_VOICE_ID=your_terry_crews_voice_id_here
ELEVENLABS_WILL_HOWARD_VOICE_ID=your_will_howard_voice_id_here
ELEVENLABS_PARKER_MUNNS_VOICE_ID=your_parker_munns_voice_id_here

# Supabase Configuration (for file storage)
SUPABASE_URL=<example URL>
SUPABASE_ANON_KEY=<anon key>
SUPABASE_SERVICE_ROLE_KEY=<service role key>

# Storage Configuration
STORAGE_BUCKET=test-bucket-ponteai
DEFAULT_REQUESTER_ID=test_user_id
PROD_STORAGE_BUCKET=<prod bucket>

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info 